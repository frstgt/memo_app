<li id="note-<%= note.id %>">
  <span class="title"><b><%= note.title %></b></span>
  <span class="description"><%= note.description %></span>
  <span class="timestamp">
    Created <%= time_ago_in_words(note.created_at) %> ago.
    Updated <%= time_ago_in_words(note.updated_at) %> ago.
  </span>
</li>
