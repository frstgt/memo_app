<section class="note_info">

  <span class="title">(note) <b><%= @note.title %></b></span>
  <span class="description"><%= @note.description %></span>

  <span class="timestamp">
    created <%= time_ago_in_words(@note.created_at) %> ago.<br>
    updated <%= time_ago_in_words(@note.updated_at) %> ago.
  </span>

</section>
    