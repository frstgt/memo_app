<span class="header">
  <span class="count">
    user notes (<%= @all_notes.count %>)
  </span>
  <span class="command">
    <%= link_to "[new]", new_unote_path %>
  </span>
</span>

<% if @all_notes.any? %>
  <ol class="notes">
    <%= render @page_notes %>
  </ol>
  <%= will_paginate @page_notes %>
<% end %>