<span class="header">

  <span class="count">
    memos (<%= @all_memos.count %>)
  </span>

  <span class="command">
    <%= link_to "[new]", new_group_gnote_gmemo_path(@group, @note) %>
  </span>

</span>

<% if @all_memos.any? %>
  <ol class="notes">
    <%= render @page_memos %>
  </ol>
  <%= will_paginate @page_memos %>
<% end %>